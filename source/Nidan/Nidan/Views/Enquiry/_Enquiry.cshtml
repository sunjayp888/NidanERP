                                </label>
                                <label class='@(Model.Enquiry.Gender == "Female" ? "btn btn-default active" : "btn btn-default")' data-toggle-class="btn-primary" data-toggle-passive-class="btn-default">
                                    @Html.RadioButtonFor(model => model.Enquiry.Gender, "Female")
                                    <input type="radio" name="gender" value="Female" data-parsley-multiple="gender"> Female
                                </label>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

