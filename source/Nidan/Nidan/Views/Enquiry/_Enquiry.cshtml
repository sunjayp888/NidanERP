<div class="col-md-12 col-sm-12 col-xs-12">
    <div class="x_panel">
        <div class="x_title">
            <h2><i class="fa fa-bars"></i> Area Of Interest <small>fill area of interest</small></h2>
            <div class="clearfix"></div>
        </div>
        <div ng-controller="CourseController as model" ng-init="model.setDefaultSector(@Model.Enquiry.SectorId)">
            <div class="x_content">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="col-lg-6">
                            <div class="form-group">
                                @Html.LabelFor(model => model.Enquiry.SchemeId, new { @class = "control-label col-md-4 col-sm-4 col-xs-12" })
                                <div class="col-md-8 col-sm-4 col-xs-12">
                                    @Html.DropDownListFor(model => model.Enquiry.SchemeId, Model.Schemes, "Select Scheme", new { @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.Enquiry.SchemeId, "", new { @class = "help-block" })
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class="form-group">
                                @Html.LabelFor(model => model.Enquiry.SectorId, new { @class = "control-label col-md-4 col-sm-4 col-xs-12" })
                                <div class="col-md-8 col-sm-4 col-xs-12">
                                    @Html.DropDownListFor(model => model.Enquiry.SectorId, Model.Sectors, "Select Sector", new { @class = "form-control", ng_model = "model.type", ng_change = "model.retrieveCourseBySectorId()"})
                                    @Html.ValidationMessageFor(model => model.Enquiry.SectorId, "", new { @class = "help-block" })
                                </div>
                            </div>
                        </div>
                    </div>
                </div>


                <div class="row">
                    <div class="col-lg-12">
                        <div class="col-lg-12">
                            <div class="form-group">
                                @Html.Label("Interested Course", new { @class = "control-label col-md-2 col-sm-4 col-xs-12" })
                                <div class="col-md-10 col-sm-4 col-xs-12">
                                    <input type="hidden" id="@Html.IdFor(model => model.SelectedCourseIdsJson)" name="@Html.NameFor(model => model.SelectedCourseIdsJson)" ng-init="model.selectedCourses = @Model.SelectedCourseIdsJson" value="{{model.selectedCourses}}" />
                                    <ui-select multiple tagging ng-model="model.selectedCourses" on-remove="model.deleteEnquiryCourse(@Model.Enquiry.EnquiryId,$item)" class="form-control" ng-change="model.filterSelected()" style="width: 100%" ng-disabled="model.courses.length <=0">
                                        <ui-select-match placeholder="Select courses"> <span ng-bind-html="$item.Name"></span></ui-select-match>
                                        <ui-select-choices repeat="course.CourseId as course in model.courses | filter:$select.search">
                                            <span ng-bind-html="course.Name"></span>
                                        </ui-select-choices>
                                    </ui-select>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-12">
                            <div class="form-group">
                                @Html.LabelFor(model => model.Enquiry.OtherInterestedCourse, new { @class = "control-label col-md-2 col-sm-4 col-xs-12" })
                                <div class="col-md-10 col-sm-4 col-xs-12">
                                    @Html.TextBoxFor(model => model.Enquiry.OtherInterestedCourse, new { @class = "form-control col-md-7 col-xs-12" })
                                    @Html.ValidationMessageFor(model => model.Enquiry.OtherInterestedCourse, "", new { @class = "help-block" })
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <div class="col-lg-6">
                        <div class="form-group">
                            @Html.LabelFor(model => model.Enquiry.BatchTimePreferId, new { @class = "control-label col-md-4 col-sm-4 col-xs-12" })
